<form action="/update" method="POST" class="input-student">
                    <div class="form-row">
                        <div class="form-group col-lg-4">
                            <label for="idStudent">ID Student</label>
                            <input type="text" maxlength="10" class="form-control" name="idStudent" id="idStudent" placeholder="ID Student" value="<%=updateStudent.studentId%>">
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-lg-4">
                            <label for="name">Name Student</label>
                            <input type="text" maxlength="200" class="form-control" name="name" id="name" placeholder="Name Student" value="<%=updateStudent.name%>">
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-lg-4">
                            <label for="dob">Date of birth</label>
                            <input type="text" maxlength="100" class="form-control" name="dob" id="dob" placeholder="Date of Birth Student" value="<%=updateStudent.dob%>">
                        </div>
                    </div>  

                    <div class="form-row">
                        <div class="form-group col-lg-4">
                            <label for="address">Address</label>
                            <input type="text" maxlength="200" class="form-control" name="address" id="address" placeholder="Adress Student" value="<%=updateStudent.address%>">
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-lg-4">
                            <label for="email">Email</label>
                            <input type="email" maxlength="200" class="form-control" name="email" id="email" placeholder="Email Student" value="<%=updateStudent.email%>">
                        </div>
                    </div> 
                    <input type="hidden" name="indexUpdate" id="indexUpdate" value="<% var index = studentList.findIndex((el)=>{return el.id == updateStudent.id}) %> <%=index%>" >
                    <button type="submit" class="btn btn-primary btn-update">Update Student</button>
</form>